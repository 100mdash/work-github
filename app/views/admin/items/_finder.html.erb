<%= form_with url: admin_finder_path, method: :get, local: :true do |f| %>
  <h5>商品検索</h5>
    <%= f.text_field :word %>
    <%= f.submit "検索" %>
    <%= f.select :search, options_for_select([["完全一致","perfect_match"],["前方一致","forward-match"],["後方一致","backword_match"],["部分一致","partial_match"]]) %>
<% end %>

